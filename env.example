# Stripe API Keys
VITE_STRIPE_API_KEY=sk_test_your_test_key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key

# Server Configuration
PORT=3000
NODE_ENV=development

# Optional Override for API Endpoint (defaults to https://api.stripe.com)
# STRIPE_API_ENDPOINT=https://api.stripe.com

# Optional API Version Override (defaults to most recent version)
# STRIPE_API_VERSION=2023-10-16

# Redis Configuration (optional - for caching)
# REDIS_URL=redis://localhost:6379
# REDIS_PASSWORD=
# REDIS_TTL=3600

# Example Webhooks
# WEBHOOK_SECRET=whsec_your_webhook_signing_secret

# Test Mode Parameters
# For v2 API Server
V2_PORT=3001
